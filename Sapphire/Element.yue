return (options={}) ->
    element = {}

    element.master = options.master
    element.id = options.id ?? ''
    element.x = options.x ?? 0
    element.y = options.y ?? 0
    element.positionMode = options.position ?? 'relative'
    element.width = options.width ?? 50
    element.height = options.height ?? 50
    element.alpha = options.alpha ?? 1
    element.color = options.color ?? { 1, 1, 1 }

    return {
        style: element,
        id: element.id,
        type: 'baseElement'
    }